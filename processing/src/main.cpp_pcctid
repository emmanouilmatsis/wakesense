#include <iostream>
#include "PCCTID/PCCTID.h"
#include "Parser/Parser.h"

using namespace std;

int main()
{
	Parser baseParser("../data/base.dat");
	Parser inputParser("../data/input.dat");
	PCCTID pcctid;

	pcctid.run(true, inputParser.getData());
	cout << "2D = " << baseParser.getId()[pcctid.getId()] << endl;

	pcctid.run(false, inputParser.getData());
	cout << "3D = " << baseParser.getId()[pcctid.getId()] << endl;

	return 0;
}
